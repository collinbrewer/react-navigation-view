  "scripts": {
    "clean": "rimraf lib",
    "copy": "copyfiles -f ./src/styles/react-navigation-view.css ./lib",
    "build_old": "npm run copy & babel --source-map inline -d lib/ src/",
    "prebuild": "npm run clean",
    "build": "npm run copy & webpack --env=lib",
    "build:watch": "webpack --watch --env=lib",
    "examples": "gulp dev:server",
    "lint": "eslint ./src",
    "publish:site": "NODE_ENV=production gulp publish:examples",
    "release:major": "npm run build && npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm run build && npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm run build && npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:lib": "node server.js --env=lib",
    "start": "npm run serve",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "test:cov": "karma start --single-run --reporters coverage"
  },
